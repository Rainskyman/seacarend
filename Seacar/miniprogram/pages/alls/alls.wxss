.fenl {
  width: 100%;
  height: 100%;
  background: #f5f5f5;
  display: flex;
  justify-content: flex-start;
}

.fenl-l {
  width: 150rpx;
  height: 100%;
  background: #F0E7DA;
  /* #F7E0DA; */
}

.left-it {
  height: 60rpx;
  padding: 5rpx;
  border-bottom: 2rpx solid #f5f5f5;
  text-align: center;
}

.left-it text {
  line-height: 60rpx;
}

.fenl-r {
  flex: 1;
  height: 100%;
  background:  #fff;
}

.shoptitle {
  width: 100%;
  height: 80rpx;
  padding: 0 10rpx;
  line-height: 80rpx;
  color: #000;
  background: #F7E0DA;
}

.shoplist {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}

.shoplist .item {
  width: 33.3%;
  height: 200rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}

.shoplist .item image {
  width: 75%;
  height: 75%;
}

.shoplist .item  text {
  width: 100%;
  text-align: center;
  color: #666;
}

